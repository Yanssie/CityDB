extend layout
block navbarcontent
  .container-fluid
    ul.nav.navbar-nav.navbar-right
      li
        a(href='New_York') Home
      li
        a(href='/setting') Setting

block content
  h3
    | Welcome! Check your details below:

  p
    table(class="table")
      tr
        td="Username"
        td #{user.username}
      tr
        td="Email"
        td #{user.email}
      tr
        td="First Name"
        td #{user.firstName}
      tr
        td="Last Name"
        td #{user.lastName}

  a(href='/signout', class='text-center new-account') Sign Out
